		$("#one.spotlight_img,#two.spotlight_img,#three.spotlight_img").fadeOut("slow");
		$("#four.spotlight_img").fadeIn("slow");
		$("#one.spotlight_thumb img,#two.spotlight_thumb img,#three.spotlight_thumb img").animate({opacity: 1},50);
		$("#four.spotlight_thumb img").animate({opacity: 0.5},50);
