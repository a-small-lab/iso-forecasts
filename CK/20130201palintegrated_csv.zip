	}
	
	 function slide2 () {
	$("#one.spotlight_img,#three.spotlight_img,#four.spotlight_img").fadeOut("slow");
		$("#two.spotlight_img").fadeIn("slow");
		$("#one.spotlight_thumb img,#three.spotlight_thumb img,#four.spotlight_thumb img").animate({opacity: 1},50);
		$("#two.spotlight_thumb img").animate({opacity: 0.5},50);	
	}
	
